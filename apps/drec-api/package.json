{
  "private": true,
  "name": "@energyweb/origin-drec-api",
  "version": "0.0.1",
  "description": "",
  "main": "dist/js/src/index.js",
  "author": "EnergyWeb DevHub GmbH;",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "nest start -p tsconfig.json",
    "start:dev": "yarn migrate && nest start --watch -p tsconfig.json",
    "start:debug": "nest start --debug --watch -p tsconfig.json",
    "start:prod": "node dist/js/src/main",
    "prebuild": "shx rm -rf dist",
    "build": "yarn build:ts",
    "build:ts": "tsc -b tsconfig.json",
    "build:containers": "make build",
    "clean": "shx rm -rf dist",
    "drop": "yarn redis:drop && yarn typeorm:drop && yarn typeorm:drop:issuer",
    "redis:drop": "redis-cli flushall",
    "deploy:heroku": "make -B build deploy-heroku",
    "prettier": "prettier --write --config-precedence file-override './src/**/*'",
    "lint": "eslint \"src/**/*{.ts,.tsx}\" \"test/**/*{.ts,.tsx}\" --no-error-on-unmatched-pattern",
    "lint-fix": "eslint \"src/**/*{.ts,.tsx}\" \"test/**/*{.ts,.tsx}\" --fix --no-error-on-unmatched-pattern",
    "migrate": "yarn typeorm:run:issuer && yarn typeorm:run",
    "migrate:docker": "yarn typeorm:run:issuer && node_modules/typeorm/cli.js --config dist/js/ormconfig.js migration:run",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js --config ormconfig-dev.ts",
    "typeorm:migrate": "yarn typeorm migration:generate -- -n",
    "typeorm:run": "yarn typeorm migration:run",
    "typeorm:run:issuer": "node_modules/typeorm/cli.js migration:run --config node_modules/@energyweb/issuer-api/dist/js/ormconfig.js",
    "typeorm:drop": "yarn typeorm schema:drop",
    "typeorm:drop:issuer": "node_modules/typeorm/cli.js schema:drop --config node_modules/@energyweb/issuer-api/dist/js/ormconfig.js",
    "typeorm:dropAndMigrate": "yarn typeorm:drop && yarn typeorm:run",
    "test:mocha": "mocha -r ts-node/register 'src/**/*.spec.ts' --timeout 60000 --exit",
    "test:e2e:mocha": "mocha -r ts-node/register test/*.e2e-spec.ts --timeout 60000 --exit",
    "test:e2e": "concurrently --success first --kill-others -n eth,test \"yarn run:ganache\" \"wait-on tcp:8545 && yarn typeorm:drop && yarn migrate && yarn test:e2e:mocha\"",
    "test:e2e:single": "sh ./scripts/test-e2e-single.sh",
    "run:ganache": "ganache-cli -q -m 'stone eyebrow bronze glide upon subject camera cushion rice outside swing casual' -l 8000000 -e 1000000 -a 20"
  },
  "bin": {
    "drec-backend": "./bin/drec-api"
  },
  "dependencies": {
    "@energyweb/energy-api-influxdb": "0.7.1",
    "@energyweb/issuer": "3.2.1-alpha.1623138698.0",
    "@energyweb/issuer-api": "0.2.1-alpha.1623138698.0",
    "@energyweb/origin-backend": "10.0.2-alpha.1623138698.0",
    "@energyweb/origin-backend-core": "8.0.2-alpha.1623138698.0",
    "@energyweb/origin-backend-utils": "1.5.2-alpha.1623138698.0",
    "@energyweb/utils-general": "11.0.3-alpha.1623138698.0",
    "@nestjs/common": "7.6.13",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.13",
    "@nestjs/cqrs": "7.0.1",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "7.1.5",
    "@nestjs/schedule": "0.4.2",
    "@nestjs/swagger": "4.7.15",
    "@nestjs/typeorm": "7.1.5",
    "@nestjs/platform-express": "7.6.13",
    "axios": "0.21.1",
    "bcryptjs": "2.4.3",
    "class-validator": "0.13.1",
    "class-transformer": "0.3.1",
    "concurrently": "5.3.0",
    "ethers": "5.0.31",
    "ganache-cli": "6.12.2",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "8.5.1",
    "pkcs7-padding": "0.1.1",
    "rijndael-js": "2.0.0",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.31",
    "dotenv": "8.2.0",
    "moment": "2.29.1",
    "qs": "6.9.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "4.17.9",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "shx": "0.3.3",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
